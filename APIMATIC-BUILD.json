{
  "$schema": "https://titan.apimatic.io/api/build/schema",
  "buildFileVersion": "1",
  "generatePortal": {
    "contentFolder": "./",
    "apiSpecPath": "./spec",
    "apiSpecs": [],
    "languageConfig": {
      "http": {},
      "csharp": {},
      "java": {},
      "python": {},
      "ruby": {},
      "php": {},
      "typescript": {}
    },
    "portalSettings": {
      "container": "apimatic-widget",
      "portalStyle": "default",
      "enableExport": true,
      "enableAnalytics": true,
      "enableConsoleCalls": true,
      "useProxyForConsoleCalls": true,
      "codegenApiRoutes": {
        "docsgen": "/static/docs/{template}.json",
        "codegen": "/static/sdks/{template}.zip",
        "transform": "/static/exports/{format}.{ext}"
      }
    },
    "pageTitle": "NASA EPIC API",
    "logoUrl": "https://apimaticio.blob.core.windows.net/images/draft-logos/favicon-192-3a0cd420-b2fb-4e53-be3b-dd5ba674671a.png",
    "logoLink": "https://epic.gsfc.nasa.gov/about/api",
    "useHostedPortalScript": true,
    "debug": {
      "publishReport": false,
      "dumpImportedApi": false,
      "reportPath": "./apimatic-debug/apimatic-report.html",
      "importedApiDumpPath": "./apimatic-debug/imported-api-dump.json"
    }
  }
}